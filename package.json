    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@babel/preset-env": "^7.8.3",
        "@babel/runtime-corejs3": "^7.8.3",
        "@types/chrome": "^0.0.74",
        "archiver": "^3.0.0",
        "babel-loader": "^8.0.2",
        "copy-webpack-plugin": "^4.5.3",
        "core-js": "^3.6.4",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.1",
        "ejs": "^2.7.4",
        "file-loader": "^1.1.11",
        "mini-css-extract-plugin": "^0.4.4",
        "node-sass": "^4.13.1",
        "sass-loader": "^7.1.0",
        "vue-loader": "^15.8.3",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-extension-reloader": "^1.1.4"
    }
